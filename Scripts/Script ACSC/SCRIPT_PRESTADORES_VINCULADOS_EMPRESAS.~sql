SELECT 
TV.CD_MULTI_EMPRESA EMPRESA_VINCULO,
PR.CD_PRESTADOR,
PR.NM_PRESTADOR,
TP.NM_TIP_PRESTA TIPO_PRESTADOR,
TCOM.DS_TIP_COMUN,
TC.DS_TIP_COMUN_PREST

FROM
PRESTADOR PR
LEFT JOIN PRESTADOR_TIPO_VINCULO TV ON TV.CD_PRESTADOR = PR.CD_PRESTADOR
INNER JOIN PRESTADOR_TIP_COMUN TC ON TC.CD_PRESTADOR = PR.CD_PRESTADOR
INNER JOIN TIP_COMUN TCOM ON TCOM.CD_TIP_COMUN = TC.CD_TIP_COMUN
INNER JOIN TIP_PRESTA TP ON TP.CD_TIP_PRESTA = PR.CD_TIP_PRESTA
WHERE PR.TP_SITUACAO = 'A'
ORDER BY 2
--AND PR.CD_PRESTADOR = 10723
--SELECT * FROM TIP_COMUN 
