select sum(a.vl_produzido_a_faturar) from tb_ft_faturamento a  
where a.cd_convenio = 68
and a.dt_competencia is not null
and a.cd_Reg_fat = 246246
and a.sn_pertence_pacote = 0

select * from reg_Fat where cd_reg_Fat = 246246

