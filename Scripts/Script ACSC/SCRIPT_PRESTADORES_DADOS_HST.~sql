select p.cd_prestador,
       p.nm_prestador,
       p.ds_codigo_conselho crm,
       p.nr_cpf_cgc,
       esp.ds_especialid
        from prestador p 
        inner join esp_med ep on ep.cd_prestador = p.cd_prestador
        inner join especialid esp on esp.cd_especialid = ep.cd_especialid
        where  p.nm_prestador in ('ANDRE LEONARDO DE SA EARP',
'ANDRE MARCHIORI',
'BEATRIZ KERN NOEL',
'BRUNNA VIANNA MARQUES BARROZO',
'CARLOS ADRIANO GAZANEGO PONTES',
'DANIEL RODRIGUES NUNES',
'EVELIN MAIA RAMIRES',
'FERNANDO SOUSA AGUIAR DE CARVALHO',
'FLAVIO ANDRIOLO MENDES',
'GUSTAVO JOSE LABRONICI',
'HAROLDO KOITI KURIKE',
'HUGO DE CASTRO SABINO',
'JOSE KAWAZOE LAZZOLI',
'JULIO MARQUES DE CARVALHO',
'LEONARDO MAGNO NEIVA DE SOUSA',
'LUCIANE MARIA DUTRA AGOSTINHO',
'NADEJDA FAINBAUM',
'NELIO GOMES JUNIOR',
'RENAN DE SOUZA CARVALHO',
'TAYENE ALBANO QUINTELLA',
'TIAGO COUTAS DE SOUZA',
'WAGNER FIGUEIRA RODRIGUES',
'VINICIUS BRASIL FREITAS CRUZ',
'Álvaro Oliveira Rodrigues',
'LIANA GOMES CASALI ABI DAUD')
  and      
        /*p.cd_prestador =17895
        and*/ ep.sn_especial_principal = 'S'
      /*  and p.cd_prestador in (17895,
