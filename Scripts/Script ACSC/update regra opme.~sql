
/*update 
EMPRESA_CONVENIO 
set tp_opme_vl_referencia = 'NOTAF-SIMPRO-TABELA' 
WHERE CD_CONVENIO IN (*/
SELECT CONV.CD_CONVENIO,
       CONV.NM_CONVENIO,
       ECONV.TP_OPME_VL_REFERENCIA FROM 
CONVENIO CONV 
INNER JOIN EMPRESA_CONVENIO ECONV ON ECONV.CD_CONVENIO = CONV.CD_CONVENIO
WHERE ECONV.SN_ATIVO = 'S'
AND ECONV.CD_MULTI_EMPRESA = 7
AND CONV.TP_CONVENIO = 'C'
--AND CONV.CD_CONVENIO IN (41)-- (59,3,5,6,8,10,12,13,14,15,16,17,18,19,22,23,24,29,34,30,46,33,35,38,39,55,56,57,41,464,9,50,53,21,26,27,62,36)
