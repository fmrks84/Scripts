
SELECT 
--COUNT(*)X,
CIR.CD_CIRURGIA,
CIR.DS_CIRURGIA  ,
CIR.CD_PRO_FAT ,
RF.CD_REG_FAT,
RF.CD_CONVENIO,
RF.CD_CON_PLA,
RF.CD_REGRA,
PF.CD_GRU_PRO,
IRF.VL_UNITARIO,
RF.CD_MULTI_EMPRESA,
ITL.CD_ATI_MED,
ITL.VL_ATO


FROM 
AVISO_CIRURGIA AC 
INNER JOIN CIRURGIA_AVISO CA ON CA.CD_AVISO_CIRURGIA = AC.CD_AVISO_CIRURGIA
INNER JOIN CIRURGIA CIR ON CIR.CD_CIRURGIA = CA.CD_CIRURGIA 
INNER JOIN REG_FAT RF ON RF.CD_ATENDIMENTO = AC.CD_ATENDIMENTO
INNER JOIN ITREG_FAT IRF ON IRF.CD_REG_FAT = RF.CD_REG_FAT 
INNER JOIN PRO_FAT PF ON PF.CD_PRO_FAT = IRF.CD_PRO_FAT
INNER JOIN ITLAN_MED ITL ON ITL.CD_REG_FAT = IRF.CD_REG_FAT
AND ITL.CD_LANCAMENTO = IRF.CD_LANCAMENTO
AND IRF.CD_PRO_FAT = CIR.CD_PRO_FAT
WHERE AC.CD_AVISO_CIRURGIA IN(246396)/* ,255363  ,256467  ,263291  ,263532  ,266195  ,267783  ,
