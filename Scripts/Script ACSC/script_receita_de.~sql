select p.cd_prestador,
       p.nm_prestador,
       p.ds_codigo_conselho,
       p.nr_cpf_cgc,
       xx.ds_especialid
       
        from esp_med x 
inner join prestador p on p.cd_prestador = x.cd_prestador
inner join especialid xx on xx.cd_especialid = x.cd_especialid
and p.sn_atuante = 'S'
and p.tp_situacao = 'A'
AND p.cd_multi_empresa = 7 
where x.cd_especialid = 50
and p.cd_prestador = 31735
