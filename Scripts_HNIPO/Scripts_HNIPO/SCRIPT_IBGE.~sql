
SELECT CD_NOTA_FISCAL,
       N.CD_CIDADE,
       N.NM_CIDADE,
       C.CD_IBGE,      
       N.DS_RETORNO_NFE,
       N.CD_ATENDIMENTO
       
  FROM NOTA_FISCAL N
INNER JOIN CIDADE C ON C.CD_CIDADE = N.CD_CIDADE   
WHERE N.CD_MULTI_EMPRESA in (2)  
AND TRUNC(N.DT_EMISSAO) >= '28/02/2022' 
--AND N.CD_STATUS = 'E'
and n.nr_nota_fiscal_nfe is null
AND DS_RETORNO_NFE LIKE 'CODIGO : 1001%'


--SELECT * FROM NOTA_fISCAL WHERE NOTA_FISCAL.NR_ID_NOTA_FISCAL = 862958


